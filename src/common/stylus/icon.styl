@import "variables"

@font-face
  font-family 'icon-sell'
  src url(icomoon-font-path + '/icon-sell.eot?w6gfap')
  src url(icomoon-font-path + '/icon-sell.eot?w6gfap#iefix') format('embedded-opentype'), url(icomoon-font-path + '/icon-sell.ttf?w6gfap') format('truetype'), url(icomoon-font-path + '/icon-sell.woff?w6gfap') format('woff'), url(icomoon-font-path + '/icon-sell.svg?w6gfap#icon-sell') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icon-sell' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-add_circle
  &:before
    content icon-add_circle
.icon-arrow_lift
  &:before
    content icon-arrow_lift
.icon-check_circle
  &:before
    content icon-check_circle
.icon-close
  &:before
    content icon-close
.icon-favorite
  &:before
    content icon-favorite
.icon-keyboard_arrow_right
  &:before
    content icon-keyboard_arrow_right
.icon-remove_circle_outline
  &:before
    content icon-remove_circle_outline
.icon-shopping_cart
  &:before
    content icon-shopping_cart
.icon-thumb_down
  &:before
    content icon-thumb_down
.icon-thumb_up
  &:before
    content icon-thumb_up

